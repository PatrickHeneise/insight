<% form_for([:admin, @course, @course_module]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :short %><br />
    <%= f.text_field :short, :size => 5 %>
  </p>
  <p>
    <%= f.label :visible %><br />
    <%= f.check_box :visible %>
  </p>
  <p>
    <%= f.label :level %><br />
    <%= f.text_field :level, :size => 2 %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :semester %><br />
    <%= f.text_field :semester, :size => 2 %>
  </p>
  <p>
    <%= f.submit button_name %>
  </p>
<% end %>